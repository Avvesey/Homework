#include <iostream>
using namespace std;
struct Date{
int year;
int month;
int day;
day (int year,int month , int day){
cin >> year >> month >> day;
int year_code = (6 + year%100 + (year%100)/4 )%7;
int month_code;
if (month = 10 || 01){
  month_code = 1;
}
else if(month = 05 ){
    month_code = 2;
}else if (month = 08){
  month_code = 3;
} else if(month = 02 || 03 || 11){
   month_code = 4;
} else if ( month = 06){
month_code = 5;
} else if (month = 09 || 12){
month_code = 6;
} else if ( month = 04 || 07){
month_code = 0;
}
return ( day + month_code + year_code)%7

};

int main() {


return 0;
}
